[[runners]]
  name = "{{ env }}_docker_executor"
  url = "https://gitlab.com"
  token = "{{ runner_token }}"
  executor = "docker"
  [runners.docker]
    tls_verify = false
    image = "python:3.8"
    privileged = false
    disable_cache = false
    volumes = ["/cache"]
    shm_size = 0
  [runners.cache]
